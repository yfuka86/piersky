<!DOCTYPE html>
<html>
<head>
  <title>Piersky</title>
  <%= stylesheet_link_tag 'welcome', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>
<body id='page-top'>
  <%= render 'shared/welcome_header' %>
  <%= render 'shared/notification_message'%>

  <%= yield %>
  <%= javascript_include_tag 'welcome/jquery', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'welcome/bootstrap.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'welcome/jquery.easing.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'welcome/grayscale', 'data-turbolinks-track' => true %>
</body>
</html>
