<!DOCTYPE html>
<html>
<head>
  <title>Piersky</title>
  <%= stylesheet_link_tag 'welcome', media: 'all' %>
  <%= csrf_meta_tags %>
  <%= favicon_link_tag 'favicon.ico' %>
</head>
<body id='page-top'>
  <%= render 'shared/welcome_header' %>
  <%= render 'shared/notification_message'%>

  <%= yield %>
  <%= render 'shared/welcome_footer' %>
  <%= javascript_include_tag 'welcome/jquery' %>
  <%= javascript_include_tag 'welcome/bootstrap.min' %>
  <%= javascript_include_tag 'welcome/jquery.easing.min' %>
  <%= javascript_include_tag 'welcome/grayscale' %>
  <%= render 'shared/ga' %>
</body>
</html>
