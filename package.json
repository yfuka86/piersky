{
  "name": "piersky",
  "private": true,
  "scripts": {
    "build": "browserify app/assets/javascripts/src/application.js -t babelify --extension=\".js.jsx\" -o app/assets/javascripts/dist/application.js",
    "watch": "watchify app/assets/javascripts/src/application.js -t babelify --extension=\".js.jsx\" -d -o app/assets/javascripts/dist/application.js -v"
  },
  "dependencies": {
    "babel": "^5.1.10",
    "babelify": "^6.0.2",
    "browserify": "~10.2.3",
    "watchify": "3.3.0",
    "eventemitter2": "^0.4.14",
    "flux": "^2.0.0",
    "superagent": "^0.21.0",
    "react-router": "0.13.3",
    "object-assign": "^1.0.0",
    "keymirror": "~0.1.0",
    "moment": "2.10.3",
    "lodash": "3.10.1",
    "bluebird": "2.9.25",
    "change-case": "2.3.0",
    "pikaday": "1.3.2",
    "react": "0.13.3",
    "react-modal": "0.2.0",
    "react-tooltip": "0.3.0",
    "d3": "3.5.6"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "2.1.x"
  }
}
